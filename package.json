{
  "name": "fashion-server",
  "version": "1.0.0",
  "description": "This is server of softypy e-commerce.",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "build": "concurrently \"tsc\" \"npm run copy-files\" ",
    "lint": "eslint src --ignore-path .eslintignore --ext .js,.ts",
    "lint-fix": "npx eslint src --fix",
    "format": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "format-fix": "npx prettier --write src",
    "make:module": "ts-node  src/app/utils/make-module.ts",
    "copy-files": "copyfiles -u 1 \"src/app/templates/**/*\" dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoftyPy-IT/e-soft-server.git"
  },
  "keywords": [
    "express",
    "node",
    "typescript",
    "javascript",
    "mongodb",
    "mongoose"
  ],
  "author": "Softypy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SoftyPy-IT/e-soft-server/issues"
  },
  "homepage": "https://github.com/SoftyPy-IT/e-soft-server#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bson": "^4.2.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/html-pdf-node": "^1.0.2",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bullmq": "^5.12.10",
    "bwip-js": "^4.3.2",
    "chalk": "^4.1.0",
    "cloudinary": "^2.2.0",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "html-pdf-node": "^1.0.8",
    "http-status": "^1.7.4",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "postcss-cli": "^11.0.0",
    "puppeteer": "^22.14.0",
    "softypy-media-service": "^2.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yaml": "^2.4.2",
    "zod": "^3.23.4"
  }
}
